<Window x:Class="RF.Temperature.WPFUI.GetData"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RF.Temperature.WPFUI"
        mc:Ignorable="d"
        Title="GetData" Height="300" Width="300">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="#FFFF9C9C" Offset="0"/>
                <GradientStop Color="#FFBD2020" Offset="1"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Button x:Name="btnGetData" Content="Get Data" HorizontalAlignment="Left" Margin="81,142,0,0" VerticalAlignment="Top" Width="120" Click="btnGetData_Click"/>
        <TextBox x:Name="txtData" HorizontalAlignment="Left" Height="23" Margin="81,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblData" Content="" HorizontalAlignment="Left" Margin="81,82,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
